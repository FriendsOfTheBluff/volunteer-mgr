<html>
<body>
<form action="/tags" method="POST">
  <div>
    <figure>
      <figcaption>Tags:</figcaption>
      <ul>
        {% for tag in tags %}
          <li>{{ tag }}</li>
        {% endfor %}
      </ul>
    </figure>
  </div>
  <div>
    <label for="new_tag">New Tag:</label>
    <input type="text" id="new_tag" name="new_tag">
  </div>
  <div class="button">
    <button type="submit">Save</button>
  </div>
</form>
</html>
</body>
